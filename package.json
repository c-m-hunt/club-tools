{
    "name": "club-tools",
    "version": "0.1.0",
    "description": "Club tools",
    "repository": {
        "type": "git",
        "url": "https://github.com/c-m-hunt/club-tools"
    },
    "author": "Chris Hunt",
    "license": "MIT",
    "scripts": {
        "start": "NODE_PATH=./src ts-node src/app.ts",
        "start-server": "NODE_PATH=./src ts-node src/slack.ts",
        "start-server-watch": "nodemon",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "test": "jest --forceExit --coverage --verbose",
        "build": "tsc -p ./tsconfig.json",
        "install:ct": "yarn build && cp ct.template ct && pwdesc=$(echo $PWD) && sed -i '' -e \"s|{{path}}|${pwdesc}|g\" ct && mv ct /usr/local/bin",
        "run:prod": "PRODUCTION=1 node -r ./tsconfig-paths-bootstrap.js dist/app.js",
        "import-members": "ts-node src/club/members/import.ts"
    },
    "dependencies": {
        "@slack/webhook": "^5.0.3",
        "@typegoose/typegoose": "^7.3.1",
        "@types/google-spreadsheet": "^3.0.0",
        "@types/inquirer": "^7.3.1",
        "@types/mongoose": "^5.7.34",
        "@types/node-fetch": "^2.5.7",
        "chalk": "^4.1.0",
        "cli-table3": "^0.6.0",
        "commander": "^6.0.0",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "google-spreadsheet": "^3.0.11",
        "inquirer": "^7.3.3",
        "mailchimp-api-v3": "^1.14.0",
        "moment": "^2.29.4",
        "mongoose": "^5.9.27",
        "node-fetch": "^2.6.1",
        "paypal-rest-sdk": "^1.8.1",
        "play-cricket-client": "^0.1.10",
        "reflect-metadata": "^0.1.13",
        "typegoose": "^5.9.1",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.27",
        "@types/body-parser": "^1.17.1",
        "@types/chai": "^4.2.3",
        "@types/compression": "^1.0.1",
        "@types/concurrently": "^4.1.0",
        "@types/errorhandler": "^0.0.32",
        "@types/eslint": "^6.1.1",
        "@types/jest": "^24.0.23",
        "@types/node": "^14.0.22",
        "@types/shelljs": "^0.8.5",
        "@types/supertest": "^2.0.8",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "concurrently": "^5.0.0",
        "eslint": "^6.4.0",
        "jest": "^24.9.0",
        "nodemon": "^2.0.4",
        "shelljs": "^0.8.3",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.6.3"
    }
}
